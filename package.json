{
	"name": "morphchat",
	"version": "1.2.9",
	"description": "A modern React chat widget component with TypeScript support",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./hooks/useChatWidget": {
			"import": "./dist/hooks/useChatWidget.js",
			"types": "./dist/hooks/useChatWidget.d.ts"
		},
		"./hooks/useChatAction": {
			"import": "./dist/hooks/useChatAction.js",
			"types": "./dist/hooks/useChatAction.d.ts"
		},
		"./components/ChatWidget": {
			"import": "./dist/components/ChatWidget.js",
			"types": "./dist/components/ChatWidget.d.ts"
		},
		"./components/page/ChatWidgetContainer": {
			"import": "./dist/components/page/ChatWidgetContainer.js",
			"types": "./dist/components/page/ChatWidgetContainer.d.ts"
		},
		"./components/page/FloatingChatButton": {
			"import": "./dist/components/page/FloatingChatButton.js",
			"types": "./dist/components/page/FloatingChatButton.d.ts"
		},
		"./components/layout/Icon": {
			"import": "./dist/components/layout/Icon.js",
			"types": "./dist/components/layout/Icon.d.ts"
		},
		"./types/WidgetConfig": {
			"import": "./dist/types/WidgetConfig.js",
			"types": "./dist/types/WidgetConfig.d.ts"
		},
		"./types/WidgetAction": {
			"import": "./dist/types/WidgetAction.js",
			"types": "./dist/types/WidgetAction.d.ts"
		},
		"./types/Theme": {
			"import": "./dist/types/Theme.js",
			"types": "./dist/types/Theme.d.ts"
		},
		"./types/Corner": {
			"import": "./dist/types/Corner.js",
			"types": "./dist/types/Corner.d.ts"
		},
		"./constants/Color": {
			"import": "./dist/constants/Color.js",
			"types": "./dist/constants/Color.d.ts"
		},
		"./dist/globals.scss": "./dist/globals.scss",
		"./globals.scss": "./dist/globals.scss"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:standalone": "vite build --config vite.config.standalone.ts",
		"build:all": "npm run build && npm run build:standalone",
		"build:lib": "tsc --project tsconfig.lib.json && tsc-alias -p tsconfig.lib.json && npm run copy-assets",
		"copy-assets": "node scripts/copy-assets.js",
		"preview": "vite preview",
		"start": "node scripts/server.js",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"clean": "rimraf dist",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"prepublishOnly": "npm run clean && npm run build:lib"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ThalesQwerty/morphchat.git"
	},
	"keywords": [
		"react",
		"typescript",
		"chat",
		"widget",
		"component",
		"ui"
	],
	"author": "ThalesQwerty",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ThalesQwerty/morphchat/issues"
	},
	"homepage": "https://github.com/ThalesQwerty/morphchat#readme",
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"sass": ">=1.60.0"
	},
	"dependencies": {
		"@phosphor-icons/react": ">=2.1.10",
		"express": ">=4.18.2",
		"lodash": ">=4.17.21",
		"openai": ">=5.12.2"
	},
	"devDependencies": {
		"@types/lodash": ">=4.17.20",
		"@types/react": ">=18.2.0",
		"@types/react-dom": ">=18.2.0",
		"@typescript-eslint/eslint-plugin": ">=6.0.0",
		"@typescript-eslint/parser": ">=6.0.0",
		"@vitejs/plugin-react": ">=4.2.0",
		"eslint": ">=8.0.0",
		"eslint-plugin-react": ">=7.33.0",
		"eslint-plugin-react-hooks": ">=4.6.0",
		"prettier": ">=3.0.0",
		"rimraf": ">=5.0.0",
		"sass": ">=1.90.0",
		"tsc-alias": "^1.8.16",
		"typescript": ">=5.0.0",
		"vite": ">=4.5.0"
	}
}
