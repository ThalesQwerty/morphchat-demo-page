{
	"name": "morphchat",
	"version": "1.0.4",
	"description": "A modern React chat widget component with TypeScript support",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:standalone": "vite build --config vite.config.standalone.ts",
		"build:all": "npm run build && npm run build:standalone",
		"build:lib": "tsc --project tsconfig.lib.json && npm run copy-assets",
		"copy-assets": "node scripts/copy-assets.js",
		"preview": "vite preview",
		"start": "node scripts/server.js",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"clean": "rimraf dist",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"prepublishOnly": "npm run clean && npm run build:lib"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ThalesQwerty/morphchat.git"
	},
	"keywords": [
		"react",
		"typescript",
		"chat",
		"widget",
		"component",
		"ui"
	],
	"author": "ThalesQwerty",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ThalesQwerty/morphchat/issues"
	},
	"homepage": "https://github.com/ThalesQwerty/morphchat#readme",
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"sass": ">=1.60.0"
	},
	"dependencies": {
		"@phosphor-icons/react": ">=2.1.10",
		"lodash": ">=4.17.21",
		"openai": ">=5.12.2"
	},
	"devDependencies": {
		"@types/lodash": ">=4.17.20",
		"@types/react": ">=18.2.0",
		"@types/react-dom": ">=18.2.0",
		"@typescript-eslint/eslint-plugin": ">=6.0.0",
		"@typescript-eslint/parser": ">=6.0.0",
		"@vitejs/plugin-react": ">=4.2.0",
		"eslint": ">=8.0.0",
		"eslint-plugin-react": ">=7.33.0",
		"eslint-plugin-react-hooks": ">=4.6.0",
		"prettier": ">=3.0.0",
		"rimraf": ">=5.0.0",
		"sass": ">=1.90.0",
		"typescript": ">=5.0.0",
		"vite": ">=4.5.0",
		"react": ">=18.2.0",
		"react-dom": ">=18.2.0",
		"express": ">=4.18.2"
	}
}
